{
  "multipart": [
    {
      "apply": {
        "model": "structurize:block/spruce_blockpaperwall_post"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "structurize:block/spruce_blockpaperwall_side"
      }
    },
    {
      "when": {
        "north": "false"
      },
      "apply": {
        "model": "structurize:block/spruce_blockpaperwall_noside"
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "structurize:block/spruce_blockpaperwall_side_alt"
      }
    },
    {
      "when": {
        "south": "false"
      },
      "apply": {
        "model": "structurize:block/spruce_blockpaperwall_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "structurize:block/spruce_blockpaperwall_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": "false"
      },
      "apply": {
        "model": "structurize:block/spruce_blockpaperwall_noside",
        "y": 270
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "structurize:block/spruce_blockpaperwall_side",
        "y": 90
      }
    },
    {
      "when": {
        "east": "false"
      },
      "apply": {
        "model": "structurize:block/spruce_blockpaperwall_noside_alt"
      }
    }
  ]
}